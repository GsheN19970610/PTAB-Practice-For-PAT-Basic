#include<stdio.h>
#include<algorithm>
using namespace std;

bool cmp(int a,int b)
{
	return a>b;
}

int main()
{
	int n,i,a[110],m;
	scanf("%d",&n);
	bool HashTable[10000]={0};
	for(i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
		m=a[i];								//这里就要用m,不能直接用a[i],我也不知道为什么,2018/11/12留.
		while(m!=1)           //这里的想法很巧妙,即将所有的数字一直运算到1,将除它本身之外的数字HashTable都设置为true
		{                     
			if(m%2==1)          
				m=(m*3+1)/2;
			else
				m=m/2;
			HashTable[m]=true;
		}
	}
	int count=0;
	for(i=0;i<n;i++)
	{
		if(HashTable[a[i]]==false)
		{
			count++;
		}
	}
	sort(a,a+n,cmp);
	for(i=0;i<n;i++)
	{
		if(HashTable[a[i]]==false)
		{
			printf("%d",a[i]);
			count--;
			if(count>0)
				printf(" ");
		}
	}
	return 0;
}
