#include<stdio.h>
int main()
{
	long a,b,c;
	int T,tcase=1;
	scanf("%d",&T);
	while(T--)
	{
	  scanf("%ld %ld %ld",&a,&b,&c);
		if(a+b>c)
		{
			printf("Case #%d: true\n",tcase);tcase++;
		}
		else
		{
			printf("Case #%d: false\n",tcase);tcase++;
		}
	}
	return 0;
}
